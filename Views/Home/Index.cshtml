<head>     
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<title>AddressBook</title>
</head> 
<body> 
	<h1>WELCOME TO YOUR ADDRESS BOOK</h1>
	<br><br>
	@foreach (var contact in Model)     
	{          
		if (contact !=null)         
		{             
			<li><strong><a href='/contact/@contact.GetId()'>@contact.GetName()</a></strong></li>
		}
	}
	<button id="addcontact" type="button" class="btn btn-primary btn-lg"><a href="/contact/create">Add Contact</a></button> 
	<button id="viewallcontact" type="button" class="btn btn-primary btn-lg"><a href="/contact/deleteall">Delete All Contacts</a></p> 
</body>