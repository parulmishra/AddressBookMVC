<head>     
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<title>AddressBook</title>
	<link rel="stylesheet" href="/css/styles.css">
</head> 
<div id="landingPage" class="well">
	<h1>WELCOME TO YOUR ADDRESS BOOK</h1>
	<br>
	<br>
	@foreach (var contact in Model)     
	{          
		if (contact !=null)         
		{             
			<li><strong><a href='/contact/@contact.GetId()'>@contact.GetName()</a></strong></li>
		}
	}
	<h2><a href="/contact/create">Add Contact</a></h2> 
	<br>
	<br>
	<h2><a href="/contact/deleteall">Delete All Contacts</a></h2> 
</div>
<style>
	#landingPage
	{
		background-color: #FFFFCC;		
		width: 50%;
		height: 100%;
	}
	form
	{
		font-size: 20px;
	}
	li
	{
		font-size: 16px;
	}
</style>
